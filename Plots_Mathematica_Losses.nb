Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"mseLoss", "[", 
      RowBox[{"x_", ",", "p_", ",", "class_"}], "]"}], ":=", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"class", "\[Equal]", "1"}], ",", 
